RewriteEngine On    # Turn on the rewriting engine
RewriteRule ^all$ get_all.php [NC,L]
RewriteRule ^get$ get_by_type.php [NC,L]
RewriteRule ^id=([0-9_-]*)$ get_by_id.php?id=$1 [NC,L]
RewriteRule ^about-page$ get_news_about_page.php [NC,L]
RewriteRule ^create$ create.php [NC,L]
RewriteRule ^delete$ delete.php [NC,L]
RewriteRule ^update$ update.php [NC,L]